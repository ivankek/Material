@model List<BatallaDeLosPenales.Servicios.Entidades.Arquero>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<h1>Arqueros:</h1>

<ul>

    @foreach (var Arquero in Model)
    {
        <li>
            <p>
                @Arquero.Id  @Arquero.Nombre @Arquero.Apellido @Arquero.PenalesAtajados @Arquero.Puntos
            </p>
        </li>
    }

</ul>
