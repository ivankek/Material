@model List<BatallaDeLosPenales.Servicios.Entidades.DirectorTecnico>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<h1>Director Técnicos:</h1>

<ul>

    @foreach (var DirectorTecnico in Model)
    {
        <li>
            <p>
                Nombre:
                @DirectorTecnico.NombreUsuario
            </p>

            <p>
                Puntos:
                @DirectorTecnico.Puntos
            </p>

        </li>
    }

</ul>

